    <script>

    window.onload = function(){

    if(navigator.language.startsWith('en')){
        var has_language_automatically_changed = localStorage.getItem('has_language_automatically_changed');
        if(has_language_automatically_changed != '1'){
                localStorage.setItem('has_language_automatically_changed','1');
                location.href =  baseUrl + "en_home/index";
        }
    }

        bimChangeApplicationLanguage();

        RequestInforMailSender();

        var sub_links = document.getElementsByClassName('has_sub_links');

        var sub_navigation_headings = document.getElementsByClassName('sub_navigation_heading');

        var desktop_menu = document.getElementById('desktop_menu');

        var menu_togglers = document.getElementsByClassName('menu_toggler');

        for(var counter_toggler=0;counter_toggler<menu_togglers.length;counter_toggler++){
            var menu_toggler = menu_togglers[counter_toggler];
            menu_toggler.onclick = function(e){
                    desktop_menu.classList.toggle('mobile_active');
            }
        }

        for(var counter_link=0;counter_link<sub_links.length;counter_link++){
            var sub_link = sub_links[counter_link];
            sub_link.onclick = function(e){
                if(e.target.classList.contains('main_link')){
                e.target.parentElement.classList.toggle('mobile_active');
                }else{
                e.target.classList.toggle('mobile_active')
                }
            }
        }

        for(var counter_heading=0;counter_heading<sub_navigation_headings.length;counter_heading++){
            var sub_navigation_heading = sub_navigation_headings[counter_heading];
            sub_navigation_heading.onclick = function(e){
                e.target.nextElementSibling.classList.toggle('mobile_active');
                e.target.parentElement.parentElement.classList.toggle('mobile_active');
            }
        }

        function RequestInforMailSender(){
            
            var request_info = document.getElementById('request_info');
            var request_firstname = document.getElementById('request_firstname');
            var request_lastname = document.getElementById('request_lastname');
            var request_email = document.getElementById('request_email');
            var request_message = document.getElementById('request_message');

            request_info.onclick = function(){
                var template_subject = "ENQUIRY REQUEST FROM WEBSITE (BIM-EDU)";
                var template_message = "<div style='padding:20px;background:#4b345d;color:white;'>" +
                            "<h1 style='color:white;font-weight:bolder;text-align:center;'>" +
                            "<span style='color:white'>" + template_subject +" </span>" +
                            "</h1>" +
                            "<div style='color:white;font-weight:bolder;text-align:center'>" +
                                "<div style='text-align:left'><span> Name </span> : " + request_firstname.value  + " " + request_lastname.value + "<span> </span></div><br/><br/>" +
                                "<div style='text-align:left'><span> Email </span style='color:white'> : " + request_email.value + "<span> </span></div><br/><br/>" +
                                "<div style='text-align:left'><span> Message </span> : " + request_message.value + "<span> </span></div><br/><br/>" +
                            "</div>" +
                            "</div>";

                var request_data = {
                    studentEmail: _enquiryemailreceiver,
                    subject : "REQUEST INFO FROM BIM-EDU WEBSITE",
                    message : template_message
                }

                $.ajax({
                        type: "GET",
                        url: baseUrl + "ApiUser/SendEmail",
                        dataType: "json",
                        data: request_data,
                        success: function(credentials){
                            alert("Request Sent");
                        },
                        error: function() { 
                            alert("Something went wrong");
                        }
                })
            }
        }
    }

    </script>

